<?xml version="1.0" encoding="UTF-8"?>
<project name="module_backedupnotes" default="compile.module.backedupnotes">
  <dirname property="module.backedupnotes.basedir" file="${ant.file.module_backedupnotes}"/>
  
  <property name="module.jdk.home.backedupnotes" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.backedupnotes" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.backedupnotes" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.backedupnotes" value="-encoding UTF-8 -source 1.7 -target 1.7 ${compiler.args}"/>
  
  <property name="backedupnotes.output.dir" value="${module.backedupnotes.basedir}/build/classes/production/BackedUpNotes"/>
  <property name="backedupnotes.testoutput.dir" value="${module.backedupnotes.basedir}/build/classes/test/BackedUpNotes"/>
  
  <path id="backedupnotes.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="backedupnotes.module.production.classpath">
    <path refid="${module.jdk.classpath.backedupnotes}"/>
  </path>
  
  <path id="backedupnotes.runtime.production.module.classpath">
    <pathelement location="${backedupnotes.output.dir}"/>
  </path>
  
  <path id="backedupnotes.module.classpath">
    <path refid="${module.jdk.classpath.backedupnotes}"/>
    <pathelement location="${backedupnotes.output.dir}"/>
  </path>
  
  <path id="backedupnotes.runtime.module.classpath">
    <pathelement location="${backedupnotes.testoutput.dir}"/>
    <pathelement location="${backedupnotes.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.backedupnotes">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.backedupnotes">
    <patternset refid="excluded.from.module.backedupnotes"/>
  </patternset>
  
  
  <target name="compile.module.backedupnotes" depends="compile.module.backedupnotes.production,compile.module.backedupnotes.tests" description="Compile module BackedUpNotes"/>
  
  <target name="compile.module.backedupnotes.production" depends="register.custom.compilers" description="Compile module BackedUpNotes; production classes"/>
  
  <target name="compile.module.backedupnotes.tests" depends="register.custom.compilers,compile.module.backedupnotes.production" description="compile module BackedUpNotes; test classes" unless="skip.tests"/>
  
  <target name="clean.module.backedupnotes" description="cleanup module">
    <delete dir="${backedupnotes.output.dir}"/>
    <delete dir="${backedupnotes.testoutput.dir}"/>
  </target>
</project>